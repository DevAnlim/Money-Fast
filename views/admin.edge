		<div class="page-wrapper">
			<div class="container pt-30" v-if="template == 'promo'">
				<div class="row">
					<div class="col-sm-12">
						<div class="panel panel-default border-panel card-view">
							<div class="panel-heading">
								<div class="pull-left">
									<h6 class="panel-title txt-dark">
										Промокоды
									</h6>
								</div>
								<div class="clearfix"></div>
							</div>
							<div class="panel-wrapper collapse in">
								<div class="panel-body">
									<div id="createPromo" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="createPromoLabel" aria-hidden="true">
										<div class="modal-dialog">
											<div class="modal-content">
												<div class="modal-header">
													<button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
													<h5 class="modal-title" id="myModalLabel">Новый промокод</h5>
												</div>
												<form>
													<div class="modal-body">
														<div class="form-group">
															<label for="recipient-name" class="control-label mb-10">Код (только английские символы):</label>
															<input type="text" class="form-control" name="code" placeholder="Код" id="promo_code">
														</div>
														<div class="form-group">
															<label for="recipient-name" class="control-label mb-10">Сумма (в монетах):</label>
															<input type="text" class="form-control" name="amount" placeholder="Сумма" id="promo_amount">
														</div>
														<div class="form-group">
															<label for="recipient-name" class="control-label mb-10">Кол-во:</label>
															<input type="text" class="form-control" name="count_use" placeholder="Кол-во" id="promo_count">
														</div>
													</div>
													<div class="modal-footer">
														<button type="button" class="btn btn-default" data-dismiss="modal">Закрыть</button>
														<button type="submit" class="btn btn-success" v-on:click="createPromo">Создать</button>
													</div>
												</form>
											</div>
										</div>
									</div>
									
									<div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="createPromoLabel" aria-hidden="true" id="editPromo">
										<div class="modal-dialog">
											<div class="modal-content">
												<div class="modal-header">
													<button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
													<h5 class="modal-title" id="myModalLabel">Редактировать промокод</h5>
												</div>
												<form action="/admin/promoSave" method="post">
													<div class="modal-body">
														<input type="hidden" value="1" name="id" id="edit_promo_id">
														<div class="form-group">
															<label for="recipient-name" class="control-label mb-10">Код (только английские символы):</label>
															<input type="text" class="form-control" name="code" placeholder="Код" value="2" id="edit_promo_promo">
														</div>
														<div class="form-group">
															<label for="recipient-name" class="control-label mb-10">Сумма (в монетах):</label>
															<input type="text" class="form-control" name="amount" placeholder="Сумма" value="" id="edit_promo_amount">
														</div>
														<div class="form-group">
															<label for="recipient-name" class="control-label mb-10">Кол-во:</label>
															<input type="text" class="form-control" name="count_use" placeholder="Кол-во" value="" id="edit_promo_count">
														</div>
													</div>
													<div class="modal-footer">
														<button type="button" class="btn btn-default" data-dismiss="modal">Закрыть</button>
														<button type="submit" class="btn btn-success" v-on:click="savePromo">Сохранить</button>
													</div>
												</form>
											</div>
										</div>
									</div>
							
									<div class="text-center">
										<a class="btn btn-success btn-rounded" data-toggle="modal" data-target="#createPromo">Создать промокод</a>
									</div>
									<table class="table table-hover display  pb-30 promo_table" role="grid" aria-describedby="datable_1_info">
										<thead>
											<tr>
												<th>Код</th>
												<th>Сумма</th>
												<th>Кол-во</th>
												<th>Действия</th>
											</tr>
										</thead>
										<tbody>
										
											<tr v-for="p in paths.promo">
												<td>@{{ p.promo }}</td>
												<td>@{{ p.amount }}</td>
												<td>@{{ p.count }}</td>
												<td class="text-center"><a class="btn btn-primary btn-rounded btn-xs" v-on:click="editPromo(p)">Редактировать</a> / <a v-on:click="deletePromo(p.id)" class="btn btn-danger btn-rounded btn-xs">Удалить</a></td>
											</tr>
				
										</tbody>
										<tfoot>
											<tr>
												<th>Код</th>
												<th>Сумма</th>
												<th>Кол-во</th>
												<th>Действия</th>
											</tr>
										</tfoot>
									</table>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
			<div class="container pt-30" v-if="template == 'bots'">
